<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.diotek.addressmanager" >

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"></uses-permission>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".InsertActivity" android:label="Insert Address"/>
        <activity android:name=".DetailAddressActivity" android:label="Detail Address"/>
        <activity android:name=".ModifyAddressActivity" android:label="Modify Address"/>
        <activity android:name=".DeleteAddressActivity" android:label="Delete Address"/>
        <activity android:name=".UploadActivity" android:label="Upload File"/>
        <activity android:name=".DownloadActivity" android:label="Download File"/>
        <activity android:name=".CloudActivity" android:label="Cloud"/>
        <activity android:name=".DeleteActivity" android:label="Download File"/>

    </application>

</manifest>
